{
  "nxConsole.generateAiAgentRules": true,
  "cSpell.words": [
    "primeng"
  ],
  "eslint.experimental.useFlatConfig": true,
  "eslint.workingDirectories": [
    {
      "directory": ".",
      "changeProcessCWD": true
    }
  ],
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "eslint.options": {
    "overrideConfigFile": "eslint.config.mjs"
  },
  "eslint.quiet": false,
  "eslint.lintTask.enable": true,
  "eslint.run": "onType",
  "sqltools.connections": [
    {
      "ssh": "Disabled",
      "previewLimit": 50,
      "server": "localhost",
      "port": 5432,
      "driver": "PostgreSQL",
      "name": "rms-postgres localhost",
      "database": "rms_local",
      "username": "rms_user",
      "password": "rms_password"
    }
  ]
}
