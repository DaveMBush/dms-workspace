{
  "nxConsole.generateAiAgentRules": true,
  "cSpell.words": [
    "primeng"
  ],
  "eslint.experimental.useFlatConfig": true,
  "eslint.workingDirectories": [
    {
      "directory": ".",
      "changeProcessCWD": true
    }
  ],
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "eslint.options": {
    "overrideConfigFile": "eslint.config.mjs"
  },
  "eslint.quiet": false,
  "eslint.lintTask.enable": true,
  "eslint.run": "onType"
}
