# DMS Workspace Environment Configuration
# Copy this file to .env and modify as needed

# Feature Flags
USE_SCREENER_FOR_UNIVERSE=false

# Database Configuration
DATABASE_URL=file:./database.db

# Server Configuration
HOST=localhost
PORT=3000

# Node Environment
NODE_ENV=development

# Optional: OpenFIGI API Key for CUSIP resolution (free tier works without key, but rate limits are higher with one)
# Get a free key at https://www.openfigi.com/api
OPENFIGI_API_KEY=

# Optional: Logging Configuration
LOG_LEVEL=info
LOG_FILE=logs/dms.log



